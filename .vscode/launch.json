    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "index debug",
            "env": {
                "NODE_ENV": "development"
            },
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "index prod",
             "env": {
                "NODE_ENV": "production"
            },
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js"
        },

        
        {
            "type": "node",
            "request": "launch",
            "name": "run-permutations",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/run-permutation.js"
        }
        {
            "type": "node",
            "request": "launch",
            "name": "run-power",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/run-power.js"
        }
    ]
}